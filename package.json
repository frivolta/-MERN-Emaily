{
  "name": "emaily",
  "version": "1.0.0",
  "description": "Emaily a survey feedback email package",
  "main": "index.js",
  "engines": {
    "node": "10.16.2",
    "npm": "6.9.0"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run webhook\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "webhook": "lt --port 5000 -s frivolta"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "cookie-session": "^1.3.3",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.19.1",
    "localtunnel": "^1.9.2",
    "lodash": "^4.17.19",
    "mongoose": "^5.6.9",
    "nodemon": "^1.19.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "path-parser": "^4.2.0",
    "sendgrid": "^5.2.3",
    "stripe": "^7.8.0"
  }
}
